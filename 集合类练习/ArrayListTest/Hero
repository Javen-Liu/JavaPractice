package ArrayListTest;

public class Hero {
    private String name;
    public int hp;

    public Hero(String n,int h){
        this.name = n;
        this.hp = h;
    }

    public Hero(String n){
        this.name = n;
    }

    public Hero(){
        this.name = null;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getName(){
        return this.name;
    }

    @Override
    public String toString(){
        return "Hero:[name:"+this.name+",hp:"+this.hp;
    }
}
